{
  "name": "react-spreadsheet",
  "version": "0.5.0",
  "description": "Simple, customizable yet performant spreadsheet for React",
  "author": "Iddan Aaronsohn <mail@aniddan.com> (https://aniddan.com)",
  "keywords": [
    "spreadsheet",
    "excel",
    "react",
    "sheet",
    "table"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "scripts": {
    "lint": "yarn run eslint .",
    "check-format": "yarn run prettier --check .",
    "format": "yarn run prettier --write .",
    "test": "jest",
    "start": "yarn run cosmos",
    "build": "rollup -c",
    "prepack": "cross-env NODE_ENV=production yarn build",
    "push-coverage": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "cosmos": "cosmos",
    "check-typing": "flow"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "fbjs": "^1.0.0",
    "hot-formula-parser": "^3.0.2",
    "unistore": "^3.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "babel-eslint": "9.x",
    "babel-jest": "^24.3.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-node-env-inline": "^0.4.3",
    "babel-preset-react-app": "^9.0.0",
    "coveralls": "^3.0.1",
    "cross-env": "^5.1.4",
    "css-loader": "^2.1.1",
    "eslint": "5.x",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "2.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "7.x",
    "flow-bin": "^0.136.0",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "prettier": "^2.1.2",
    "react": "^16.14.0",
    "react-cosmos": "^4.8.1",
    "react-dom": "^16.14.0",
    "react-select": "^1.2.1",
    "rollup": "^2.32.1",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-typescript2": "^0.29.0",
    "style-loader": "^0.23.1",
    "typescript": "^4.0.5",
    "webpack": "^4.29.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iddan/react-spreadsheet.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iddan/react-spreadsheet/issues"
  },
  "homepage": "https://github.com/iddan/react-spreadsheet#readme"
}
