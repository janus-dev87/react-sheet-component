{
  "name": "@iddan/react-spreadsheet",
  "version": "0.1.2",
  "description": "Simple, customizable yet performant spreadsheet for React",
  "author": "Iddan Aaronsohn <mail@aniddan.com> (https://aniddan.com)",
  "keywords": ["spreadsheet", "excel", "react", "sheet", "table"],
  "main": "dist/Spreadsheet.js",
  "scripts": {
    "start": "NODE_ENV=development yarn build -w & yarn storybook",
    "build":
      "rm dist/*; flow gen-flow-files src --out-dir dist; babel src --out-dir dist --copy-files",
    "prepublish": "NODE_ENV=production yarn build",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "dependencies": {
    "babel-core": "^7.0.0-0",
    "classnames": "2.2.5",
    "scroll-into-view-if-needed": "1.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.38",
    "@babel/core": "^7.0.0-beta.38",
    "@storybook/react": "^3.3.13",
    "babel-eslint": "^8.2.1",
    "babel-preset-react-app": "^4.0.0-next.9754a231",
    "eslint": "^4.16.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "flow-bin": "^0.63.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iddan/react-spreadsheet.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iddan/react-spreadsheet/issues"
  },
  "homepage": "https://github.com/iddan/react-spreadsheet#readme"
}
