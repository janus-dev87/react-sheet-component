{
  "name": "@iddan/react-spreadsheet",
  "version": "0.1.11",
  "description": "Simple, customizable yet performant spreadsheet for React",
  "author": "Iddan Aaronsohn <mail@aniddan.com> (https://aniddan.com)",
  "keywords": [
    "spreadsheet",
    "excel",
    "react",
    "sheet",
    "table"
  ],
  "main": "dist/SpreadsheetStateProvider.js",
  "scripts": {
    "test": "jest --watch --verbose",
    "start": "cross-env NODE_ENV=development yarn storybook",
    "build": "rm dist/*; flow gen-flow-files src --out-dir dist; babel src --out-dir dist --copy-files",
    "prepublish": "cross-env NODE_ENV=production yarn build",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "clipboard-polyfill": "^2.4.6",
    "hot-formula-parser": "^2.3.3",
    "unistore": "^3.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.38",
    "@babel/core": "^7.0.0-beta.44",
    "@storybook/react": "^3.4.0",
    "@storybook/storybook-deployer": "^2.3.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.4.3",
    "babel-preset-react-app": "^4.0.0-next.9754a231",
    "cross-env": "^5.1.4",
    "eslint": "^4.16.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "flow-bin": "^0.73.0",
    "jest": "^22.4.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iddan/react-spreadsheet.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iddan/react-spreadsheet/issues"
  },
  "homepage": "https://github.com/iddan/react-spreadsheet#readme"
}
